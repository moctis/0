local args={...}
local t={}
if #args > 0 then
 t=lib.split(args[1])
 for i=2,#args do
  t[#t+i-1]=args[i]
 end
end

if t[6] then shell.run(t[0],t[1],t[2],t[3],t[4],t[5],t[6])
elseif t[5] then shell.run(t[0],t[1],t[2],t[3],t[4],t[5])
elseif t[4] then shell.run(t[0],t[1],t[2],t[3],t[4])
elseif t[3] then shell.run(t[0],t[1],t[2],t[3])
elseif t[2] then shell.run(t[0],t[1],t[2])
elseif t[1] then shell.run(t[0],t[1])
elseif t[0] then shell.run(t[0]) end
