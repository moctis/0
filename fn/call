function fn.call(cmd, ...)
  if (fn[cmd]) then
    local ret = { pcall(function() return fn[cmd](...) end ) }
    if ret[1] then
      local n=1
      while (ret[n+1] ~= nil) do 
        print(tostring(ret[n+1])) 
      end
    else 
      print(ret[2])
    end    
  end
end

local arg={...}
if #arg > 0 then
 fn.call(arg[1],unpack(arg,2))
end
