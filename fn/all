function fn.all(...)
 local ans={}
 local count=0
 local cmd=fn.join({...})
 print(cmd)
 fn.broadcast(cmd)
 status(cmd.."  "..count.."/"..maxDrone)
 while true do
  local i,m,d=fn.recv(2)
  if i==nil then break end
  ans['com'..i]=m
  count=count+1
  status(cmd.."  "..count.."/"..maxDrone)
 end
 return ans
end
